      <e><p><l>tetã<s n="n"/></l><r>país<s n="n"/><s n="m"/></r></p><par n="n_n"/></e>
      <e><p><l>jyva<s n="n"/></l><r>brazo<s n="n"/><s n="m"/></r></p><par n="n_n"/></e>
      <e><p><l>óga<s n="n"/></l><r>casa<s n="n"/><s n="f"/></r></p><par n="n_n"/></e>
      <e><p><l>yvyra<s n="n"/></l><r>árbol<s n="n"/><s n="m"/></r></p><par n="n_n"/></e>
      <e><p><l>tape<s n="n"/></l><r>camino<s n="n"/><s n="m"/></r></p><par n="n_n"/></e>
      <e><p><l>aravo<s n="n"/></l><r>hora<s n="n"/><s n="f"/></r></p><par n="n_n"/></e>
      <e><p><l>tata<s n="n"/></l><r>fuego<s n="n"/><s n="m"/></r></p><par n="n_n"/></e>
