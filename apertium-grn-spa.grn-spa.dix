      <e><p><l>ysyry<s n="n"/></l><r>río<s n="n"/><s n="m"/></r></p><par n="n_n"/></e>
      

      <e><re>[.\?;:!…¿¶]</re><p><l><s n="sent"/></l><r><s n="sent"/></r></p></e>
      <e><re>,</re>          <p><l><s n="cm"/></l>  <r><s n="cm"/></r></p></e>
   </section>
</dictionary>

