      <e><p><l>kavaju<s n="n"/></l><r>caballo<s n="n"/><s n="m"/></r></p><par n="n_n"/></e>
      <e><p><l>tarave<s n="n"/></l><r>cucaracha<s n="n"/><s n="f"/></r></p><par n="n_n"/></e>
      <e><p><l>ka'i<s n="n"/></l><r>mono<s n="n"/></r></p><par n="mf_GD"/></e>
